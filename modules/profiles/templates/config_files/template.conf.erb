# Configured by puppet <%= scope.source.name %>

<%-# Output Linux Distro -%>
<%= @os['distro']['id'] %>

inital_processes=<%= @num_processes %>

<%- if @interfaces.each -%>
	<%- @interfaces.each do |name, data| -%>
	<%-# Sample debugging output -%>
	<%- scope.function_warning(["name:",name]) -%>
<%= name %>:
  type <%= data['type'] %>
	<%- end -%>
<%- end -%>
